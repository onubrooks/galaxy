{
  "expo": {
    "name": "Leedder",
    "description": "A song sharing social app.",
    "slug": "Leedder",
    "privacy": "unlisted",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon-edit.png",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.artinict.leedder",
      "infoPlist": {
        "NSAppleMusicUsageDescription": "This enables the app to upload a song to your feed for other users to listen to and like.",
        "NSPhotoLibraryUsageDescription": "Firstly, enable Leedder upload a cover art for your song. ALso, allow Leedder to upload a profile picture that is displayed on your profile."
      }
    },
    "android": {
      "package": "com.artinict.leedderbuilds"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#FFFFFF"
    }
  }
}
